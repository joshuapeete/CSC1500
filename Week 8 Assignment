#Number 9
x=1.23
y=4.5

#Number 10
add = x+y
sub = x-y
multi = x*y
divi = x/y

#Number 11
f'{x} plus {y} equals {add}'
print(x,"minus",y,"equals",sub)
print("%s times %s equals %s" %(x,y,multi))
print(str(x)+" divided by "+str(y)+" equals "+str(divi))

#Number 12
import math
sq_root=math.sqrt(multi)
print("the square root of",multi,"equals",sq_root) 

